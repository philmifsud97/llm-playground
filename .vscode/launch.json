{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Documentation Helper",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/documentation-helper/ingestion.py",
            "console": "integratedTerminal",
            "python":"${command:python.interpreterPath}",
            "envFile": ".env"
        },
        {
            "name": "VectorStore in Memory",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/vectorstore-in-memory/main.py",
            "console": "integratedTerminal",
            "python":"${command:python.interpreterPath}",
            "justMyCode": true,
            "envFile": ".env"
        },
        {
            "name": "Intro Vector DB",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/intro-to-vector-db/main.py",
            "console": "integratedTerminal",
            "python":"${command:python.interpreterPath}",
            "justMyCode": true,
            "envFile": ".env"
        },
        {
            "name": "Python: RunServer",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/ice_breaker/app.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "envFile": ".env"
        },
        {
            "name": "Ice breaker runner",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${file}",
            "console": "integratedTerminal",
            "envFile": ".env"
